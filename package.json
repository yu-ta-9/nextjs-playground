{
  "name": "nextjs-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3033",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "ts:test": "tsc --noEmit",
    "db:studio": "dotenv -e .env.local npx prisma studio",
    "db:generate": "dotenv -e .env.local npx prisma generate",
    "db:push": "dotenv -e .env.local npx prisma db push",
    "db:migrate:local": "dotenv -e .env.local npx prisma migrate dev",
    "db:migrate:resolve": "dotenv -e .env.local npx prisma migrate resolve",
    "db:migrate:reset": "dotenv -e .env.local npx prisma migrate reset",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@types/node": "20.4.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "eslint-config-next": "14.2.2",
    "firebase": "^10.1.0",
    "next": "14.2.2",
    "postcss": "8.4.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@types/firebase": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.9.0",
    "dotenv-cli": "^7.2.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prisma": "^5.0.0"
  }
}
